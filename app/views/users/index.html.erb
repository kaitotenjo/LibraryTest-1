<section class="page-banner services-banner">
    <div class="container">
        <div class="banner-header">
            <h2>Sign Up</h2>
        </div>
        <div class="breadcrumb">
            <ul>
                <li><%= link_to "Home", root_url %></li>
                <li>Users</li>
            </ul>
        </div>
    </div>
</section>
